{
    "env": {
      "browser": true,
      "es6": true,
      "node": true,
      "jest": true
    },
    "extends": "airbnb",
    "parserOptions": {
      "ecmaFeatures": {
        "jsx": true
      },
      "sourceType": "module"
    },
    "plugins": [
      "react"
    ],
    "rules": {
      "linebreak-style": "off",
      "comma-dangle": ["warn", "never"],
      "arrow-body-style": "off"
    }
  }
  