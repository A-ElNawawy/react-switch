{
    "env": {
      "browser": true,
      "es6": true,
      "jest": true
    },
    "extends": "airbnb",
    "parser": "babel-eslint",
    "parserOptions": {
      "ecmaFeatures": {
        "jsx": true
      },
      "sourceType": "module"
    },
    "rules": {
      "comma-dangle": ["warn", "never"],
      "no-mixed-operators": "off",
      "object-curly-newline": "off",
      "jsx-a11y/click-events-have-key-events": "off",
      "jsx-a11y/label-has-for":[
        2,
        {
        "required": {
          "every": [ "id" ]
        }
      }]
    }
  }
  